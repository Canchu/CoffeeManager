{
  "name": "CoffeeManager",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "node-dev ./bin/www & npm run watch:scss",
    "build:scss": "node-sass ./scss --output ./public/stylesheets",
    "watch:scss": "node-sass ./scss --output ./public/stylesheets -w"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "dateformat": "^1.0.12",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "moment": "^2.15.1",
    "morgan": "~1.6.1",
    "mysql": "^2.11.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "node-sass": "^3.10.1"
  }
}
